{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "age": {
        "type": "integer",
        "minimum": 0
      },
      "role": {
        "type": "string",
        "enum": [
          "ADMIN",
          "USER"
        ]
      }
    },
    "required": [
      "name",
      "age",
      "role"
    ],
    "additionalProperties": false
  }
}